@model Dictionary<string,string>
@{
    ViewData["Title"] = "ContactUs";
}
<!--step 15 -->
<h1>Contact Us</h1>

<p>Here’s how you can reach us:</p>
<table class="table table-striped"> 
@foreach (var item in @Model)
{
    <tr>
        <td>@item.Key</td>
        <td>@item.Value</td>
    </tr>
}
</table>